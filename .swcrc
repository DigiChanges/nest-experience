{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "module": {
    "type": "commonjs"
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "baseUrl": "./",
    "paths": {
     "@src/*": ["./src/*"],
     "@config/*": ["./src/config/*"],
     "@shared/*": ["./src/shared/*"],
     "@modules/*": ["./src/modules/*"],
    },
  },
  "minify": false,
}
