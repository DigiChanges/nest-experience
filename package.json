{
  "name": "nest-experience",
  "version": "0.0.1",
  "description": "",
  "author": "DigiChanges",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "cruiser": "depcruise src",
    "start": "nest start",
    "start:dev": "nest start -w",
    "start:dev:check": "nest start --type-check -w",
    "start:debug": "nest start --type-check --debug -w",
    "start:prod": "node dist/main",
    "command": "node ./dist/command.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ts:check": "tsc -p tsconfig.build.json --noEmit",
    "pre:build": "rimraf dist",
    "pre:check": "pnpm ts:check && pnpm lint",
    "prepare": "npx husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:cov:check": "vitest run --coverage -- --CHECK",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config vitest.config.e2e.js",
    "test:ui": "vitest --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-fastify": "^10.3.10",
    "@supabase/supabase-js": "^2.44.4",
    "dayjs": "^1.11.11",
    "dependency-cruiser": "^16.4.0",
    "fastify-qs": "^4.0.2",
    "joi": "^17.13.3",
    "jwt-simple": "^0.5.6",
    "mongoose": "^8.5.1",
    "nest-commander": "^3.14.0",
    "qs": "^6.12.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.10",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.3.1",
    "@types/qs": "^6.9.15",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^2.0.3",
    "@vitest/ui": "^2.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^10.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.0.3"
  }
}
